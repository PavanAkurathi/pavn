{
    "name": "@repo/config",
    "version": "0.0.0",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./cors": {
            "types": "./dist/cors.d.ts",
            "import": "./dist/cors.js",
            "require": "./dist/cors.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "type-check": "tsc --noEmit"
    },
    "devDependencies": {
        "@repo/typescript-config": "workspace:*",
        "@types/node": "^20.14.0",
        "typescript": "^5.4.5",
        "tsup": "^8.0.0"
    }
}
