{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "bunVersion": "1.x",
    "buildCommand": "node build.mjs",
    "outputDirectory": "dist",
    "installCommand": "cd ../.. && bun install",
    "functions": {
        "dist/index.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/dist/index.js"
        }
    ]
}