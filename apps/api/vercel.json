{
    "buildCommand": "cd ../.. && bun install",
    "installCommand": "echo 'Using workspace install'",
    "framework": null,
    "outputDirectory": ".",
    "functions": {
        "api/index.ts": {
            "runtime": "bun@1",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}
